{
  "name": "@acme/nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm with-env next build",
    "start": "pnpm with-env next start",
    "clean": "rm -rf .next .turbo node_modules",
    "dev": "pnpm with-env next dev",
    "lint": "SKIP_ENV_VALIDATION=1 next lint",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "cy:open": "cypress open",
    "cy:e2e": "cypress run --browser chrome",
    "generate-pwa-asset": "node scripts/pwa-asset-generator.mjs",
    "ladle:dev": "pnpm with-env ladle serve",
    "ladle:build": "pnpm with-env ladle build",
    "ladle:preview": "pnpm with-env ladle preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "preview-coverage": "vite preview --outDir coverage"
  },
  "dependencies": {
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.7.0",
    "@codemirror/language-data": "^6.3.1",
    "@codemirror/view": "^6.12.0",
    "@headlessui/react": "^1.7.14",
    "@ladle/react": "^2.12.3",
    "@react-aria/ssr": "^3.6.0",
    "@tanstack/react-query": "^4.29.7",
    "@uiw/codemirror-theme-vscode": "^4.20.2",
    "@uiw/react-codemirror": "^4.20.2",
    "ajv": "^8.12.0",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "jotai": "^2.1.0",
    "kbar": "0.1.0-beta.40",
    "next": "^13.4.3",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "styled-components": "^5.3.10",
    "zod": "^3.21.4"
