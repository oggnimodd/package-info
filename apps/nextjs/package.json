    "@vitest/ui": "^0.31.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "cypress": "13.1.0",
    "dotenv-cli": "^6.0.0",
    "postcss": "^8.4.23",
    "pwa-asset-generator": "^6.3.1",
    "query-string": "7.1.3",
    "react-router-dom": "^6.11.2",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vitest": "^0.31.1"
